package B2;

import java.util.Scanner;

public class 분해합_2231 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		for(int i=1; i<N; i++) {
			int n = i;
			int tmp = 0;
			while(n > 0) {
				tmp += n % 10;
				n /= 10;
			}
			if((tmp + i) == N) {
				System.out.println(i);
				return;
			}
		}
		sc.close();
	}
}
